# Deklarasi Himpunan
nilai = set({3,6,9,2,5,7}) 

# Menambahkan elemen baru ke dalam himpunan
nilai.update([1,4,8,10]) 


# Menghapus elemen 1,3,4,5,7,8,dan 10
nilai.remove(1)
nilai.remove(3)
nilai.remove(4)
nilai.remove(10)
nilai.remove(7)
nilai.remove(8)
nilai.remove(5)

# Mencetak himpunan
print(nilai)
